/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=abNanded); // 1&1=0, always 1 otherwise
    Nand(a=abNanded, b=abNanded, out=out); // 0 & 0 = 1
}
